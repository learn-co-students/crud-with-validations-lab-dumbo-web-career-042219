<h1>Songs</h1>

<%= link_to "Create a Song", new_song_path %>



      <table>

     <thead>
          <tr>
            <th>Title</th>
            <th>Year</th>
            <th>Artist Name</th>
            <th>Genre</th>

          </tr>
               </thead>
          <tbody>
            <% @songs.each do |song| %>
          <tr>
            <td><%= link_to song.title, song %></td>
            <td><%= song.release_year %></td>
            <td><%= song.artist_name %></td>
            <td><%= song.genre %></td>
          </tr>
          <% end %>
        </tbody>
          </table>
